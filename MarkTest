package antgame;

import org.junit.After;
import org.junit.AfterClass;
import org.junit.Before;
import org.junit.BeforeClass;
import org.junit.Test;
import static org.junit.Assert.*;

/**
 *
 * @author Chris
 */
public class MarkTest {
    
    public MarkTest() {
    }
    
    @BeforeClass
    public static void setUpClass() {
    }
    
    @AfterClass
    public static void tearDownClass() {
    }
    
    @Before
    public void setUp() {
    }
    
    @After
    public void tearDown() {
    }

    /**
     * Test of getI method, of class Mark.
     */
    @Test
    public void testGetI() {
        System.out.println("getI");
        Mark instance = new Mark(0, 4);
        int expResult = 0;
        int result = instance.getI();
        assertEquals(expResult, result);
    }

    /**
     * Test of getSt method, of class Mark.
     */
    @Test
    public void testGetSt() {
        System.out.println("getSt");
        Mark instance = new Mark(0, 4);
        int expResult = 4;
        int result = instance.getSt();
        assertEquals(expResult, result);
    }

    /**
     * Test of toString method, of class Mark.
     */
    @Test
    public void testToString() {
        System.out.println("toString");
        Mark instance = new Mark(0, 4);
        String expResult = "Mark";
        String result = instance.toString();
        assertEquals(expResult, result);
    }
    
}
